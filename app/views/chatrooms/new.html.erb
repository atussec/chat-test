<%= form_for @chatroom do |f| %>
  <%= render "shared/error_messages", object: @chatroom %>
  <div class="row">

  <div class="input-field col s12">
    <label for="fenter_subject">Enter reciepient</label>
    <div class="">
            <%= f.select :reciepient, User.all - [current_user]).collect { |p| [ p.username, p.id ] } %>
    </div>



  </div>


  <%= f.button "create conversation", class: "waves-effect waves-light btn loginButton" %>

</div>

<% end %>
